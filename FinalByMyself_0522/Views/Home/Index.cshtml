@{
    //@model ICollection<Project>可以不写，没有实际作用
}

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 >Welcome To JBZ(Glabal machine AI Group) Support.</h1>
    <h1 >How Can we Help You?</h1>

    <ul style="list-style-type: none;">
        @foreach(var project in Model)
        {
            <li style="color:red;font-size:50px">
                <a asp-route-projectid=@project.Id asp-route-projectname=@project.Name asp-action="Index" asp-controller="Ticket">
                    @project.Name
                </a>
            </li>
        }
    </ul>

</div>
